#include <iostream>
#include <conio.h>
#define FIL 10
#define COL 10

struct tvector
{
	int nvec;
	int vec[10];
};

using namespace std;


int generar_fibonacci (int n)
{
	int i,a,b,c;
	a=0;
	b=1;
	c=1;
	for (i=0; i<n; i++)
	{
		a=b;
		b=c;
		c=c+a;
	}
	return a;
}
void cargar_columnas (tvector mat[FIL][COL], int n, int m)
{
	int i,j,k,f;
	for(i=0; i<n; i++)
	{
		for (j=0; j<m; j++)
		{
			cout << "Ingrese el tamaÃ±o: ";
			cin >> mat[j][i].nvec;

			for (k=0; k<mat[j][i].nvec; k++)
			{
				mat[j][i].vec[k]=generar_fibonacci(k+1);

			}
		}
	}

}




void main()
{
	int a,b,s;
	tvector mat[FIL][COL];
	cout << "Ingrese el numero de filas: ";
	cin >> a;
	cout << "Ingrese el numero de columnas: ";
	cin >> b;
	cargar_columnas (mat,a,b);

getch();
}
