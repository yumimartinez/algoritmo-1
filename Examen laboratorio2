#include<iostream>
#include <conio.h>
#define FIL 10
#define COL 10

struct tmatriz
{
	int mat[FIL][COL];
};

using namespace std;

int fibonacci (int n)
{
	int i,a=0,b=0, c=1;
	for (i=0; i<n; i++)
	{
		a=b;
		b=c;
		c=a+c;
	}
	return a;
}

bool verif_primo (int n)
{
	bool k=true;
	int i;
	for (i=2; i<n-1; i++)
	{
		if (n%i==0)
		{
			k=false;
		}
	}
	return k;
}

int primo (int n)
{
	int a=0,b=0;
	while (b<n)
	{
		a++;
		if(verif_primo(a)==true)
		{
			b++;
		}
	}
	return a;

}


void cargar (tmatriz vec[], int n)
{
	int i,j,a=1,b=1,l,m,k=0;
	for (i=0; i<n; i++)
	{
		for (j=0; j<n; j++)
		{
			if(k==0)
			{
				for (l=0; l<n; l++)
				{
					vec[i].mat[j][l]=primo(a);
					a++;
				}
				k=1;
			}

			else
			{
				for (m=0; m<n; m++)
				{
					vec[i].mat[j][m]=fibonacci(b);
					b++;
				}
				k=0;
			}
		}
	}
}

void mostrar (tmatriz vec[], int n)
{
	int i,j,m,l;
	for (i=0; i<n/2; i++)
	{
		for (j=0; j<n; j++)
		{
			for (l=0; l<n; l++)
			{
				cout << vec[i].mat[j][l] << " ";
			}
			cout << endl;
	
			for (m=0; m<n; m++)
			{
				cout << vec[i].mat[j][m] << " ";
			}
			cout << endl;
		}
		cout << endl;
	}

}

void main()
{
	int a;
	tmatriz vec[50];
	cout << "Ingrese el tamaÃ±o: ";
	cin >> a;
	cargar (vec,a);
	mostrar (vec,a);


getch();
}
